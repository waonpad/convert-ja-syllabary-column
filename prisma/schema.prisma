generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DB_PRISMA_URL")
}

model Post {
  id        String   @id @default(cuid())
  input     String
  column    String
  converted String
  createdAt DateTime @default(now())
}
